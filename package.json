{
  "name": "mithril-webpack-starter",
  "description": "A simple boilerplate for prototyping ES6 web apps using Mithril.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint src tools",
    "setup": "babel-node -e \"require('./tools/setup').run().catch(err => console.error(err.stack))\"",
    "bundle": "babel-node -e \"require('./tools/bundle').run().catch(err => console.error(err.stack))\"",
    "build": "babel-node -e \"require('./tools/build').run().catch(err => console.error(err.stack))\"",
    "browser-sync": "babel-node -e \"require('./tools/browserSync').run().catch(err => console.error(err.stack))\"",
    "start": "babel-node -e \"require('./tools/start')()\""
  },
  "dependencies": {
    "babel-runtime": "~6.2.0",
    "mithril": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.1",
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.3.3",
    "babel-plugin-mjsx": "^1.0.3",
    "browser-sync": "^2.10.0",
    "cpr": "^1.0.0",
    "css-loader": "^0.23.0",
    "del": "^2.1.0",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "~3.10.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "loader-utils": "^0.2.12",
    "mkdirp": "^0.5.1",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "source-map": "^0.5.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
